				<repository>
					<id>raptor-releases</id>
					<url>https://ebaycentralProxy-b3bgr.vip.slc07.dev.ebayc3.com/content/repositories/releases/</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</repository>
				<repository>
					<id>raptor-snapshots</id>
