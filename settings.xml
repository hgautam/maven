<?xml version="1.0" encoding="UTF-8"?>
<settings>
        <localRepository>${user.home}/.m2/raptor2</localRepository>
        <servers>
                <server>
                        <id>raptor.releases</id>
                        <username>RaptorTeam</username>
                        <password>RaptorTeam</password>
                </server>
                <server>
                        <id>raptor.snapshots</id>
                        <username>RaptorTeam</username>
                        <password>RaptorTeam</password>
                </server>
        </servers>

      <mirrors>
		<mirror>
			<id>central</id>
			<mirrorOf>central</mirrorOf>
			<url>https://ebaycentralProxy-b3bgr.vip.slc07.dev.ebayc3.com/content/repositories/central</url>
		</mirror>
	</mirrors>

	<profiles>
		<profile>
